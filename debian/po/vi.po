#: ../partman-crypto.templates:140
msgid "Device-mapper (LUKS)"
msgstr "Bộ ánh xạ thiết bị (LUKS)"

#. Type: text
#. Description
#: ../partman-crypto.templates:144
msgid "Passphrase"
msgstr "Cụm từ mật khẩu"

#. Type: text
#. Description
#: ../partman-crypto.templates:148
msgid "Keyfile (GnuPG)"
msgstr "Tập tin khóa (GnuPG)"

